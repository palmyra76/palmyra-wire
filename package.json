{
  "name": "@palmyralabs/palmyra-wire",
  "private": false,
  "version": "1.0.1",
  "author": "Raja K <raja@palmyralabs.com>",
  "homepage": "https://github.com/palmyra76/palmyra-wire",
  "license": "MIT",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "build": "tsc --p ./build.tsconfig.json && vite --config build.vite.config.js build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/palmyra76/palmyra-wire.git"
  },
  "peerDependencies": {
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitest/coverage-v8": "^2.0.4",
    "serve-static": "^1.15.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vite-plugin-dts": "^3.9.0",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "vitest": "^2.0.4"
  },
  "overrides": {
    "glob": "^11",
    "eslint": "^9"
  },
  "sideEffects": [
    "**/*.css"
  ]
}